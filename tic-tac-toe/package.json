{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "description": "Build a Tic Tac Toe Game project from FreeCodeCamp",
  "repository": "https://github.com/marcusmonteiro/free-code-camp-projects.git",
  "main": "./app/app.js",
  "scripts": {
    "start": "npm run dist && ./node_modules/.bin/http-server ./dist",
    "start:dev": "npm run dist && webpack-dev-server",
    "clean": "babel-node clean.js",
    "dist": "npm run clean && webpack",
    "test": "npm run lint && npm run test:unit",
    "lint": "standard | snazzy",
    "test:unit": "babel-tape-runner ./app/**/*-test.js | faucet"
  },
  "keywords": [
    "FreeCodeCamp",
    "frontend",
    "tic tac toe"
  ],
  "author": "Marcus Vinicius Monteiro de Souza",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "html-webpack-plugin": "^2.15.0",
    "html-webpack-template": "^4.0.0",
    "http-server": "^0.9.0",
    "react": "^0.14.8",
    "react-bootstrap": "^0.28.5",
    "react-dom": "^0.14.8",
    "rmdir": "^1.2.0",
    "webpack": "^1.12.14"
  },
  "devDependencies": {
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.6.0",
    "faucet": "0.0.1",
    "react-addons-test-utils": "^0.14.8",
    "snazzy": "^3.0.0",
    "standard": "^6.0.8",
    "tape": "^4.5.1",
    "webpack-dev-server": "^1.14.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
